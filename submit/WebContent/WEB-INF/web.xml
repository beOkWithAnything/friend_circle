<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>submit</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
    <servlet>
    <servlet-name>getMessageServlet</servlet-name>
    <servlet-class>com.servlet.getMessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getMessageServlet</servlet-name>
    <url-pattern>/servlet/getMessageServlet</url-pattern>
  </servlet-mapping>
  
      <servlet>
    <servlet-name>pushMessageServlet</servlet-name>
    <servlet-class>com.servlet.pushMessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pushMessageServlet</servlet-name>
    <url-pattern>/servlet/pushMessageServlet</url-pattern>
  </servlet-mapping>
  
  
</web-app>